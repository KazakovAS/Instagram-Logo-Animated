{
    "name": "template",
    "version": "1.8.0",
    "description": "WebpackTemplate",
    "private": true,
    "scripts": {
        "dev": "webpack-dev-server --config webpack.dev.config.js",
        "build": "webpack --config webpack.prod.config.js --quiet --progress --hide-modules",
        "test": "editorconfig-checker",
        "deploy": "cd build && rsync --archive --compress --delete lerush@lerush.beget.tech:test/public_html/",
        "lint": "eslint ./ -c .eslintrc --fix --ext .js,.vue && prettier ./ -c .prettierrc --write"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*(js||vue)": [
            "yarn lint",
            "node_modules/.bin/eslint â€”max-warnings=0",
            "git add"
        ]
    },
    "browserslist": {
        "production": [
            "last 3 versions",
            "Firefox >= 52",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "last 1 ie version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.5.4",
        "@babel/preset-env": "^7.5.4",
        "autoprefixer": "^9.8.4",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.0.6",
        "cache-loader": "^4.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.3",
        "css-loader": "^3.0.0",
        "cssnano": "^4.1.10",
        "csv-loader": "^3.0.2",
        "editorconfig-checker": "^3.1.0",
        "eslint": "^7.4.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-import-resolver-webpack": "^0.12.2",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^23.18.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-prettier-vue": "^2.1.1",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-vue": "^6.2.2",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.3.0",
        "husky": "^4.2.5",
        "imagemin-webpack-plugin": "^2.4.2",
        "lint-staged": "^10.2.11",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.12.0",
        "papaparse": "^5.1.1",
        "postcss-flexbugs-fixes": "^4.2.1",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.0.5",
        "purgecss-webpack-plugin": "^2.3.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^1.2.1",
        "thread-loader": "^2.1.3",
        "unused-webpack-plugin": "^2.4.0",
        "vue-eslint-parser": "^7.2.0",
        "webpack": "^4.35.3",
        "webpack-cli": "^3.3.6",
        "webpack-dev-server": "^3.4.1",
        "webpack-import-glob-loader": "^1.6.3",
        "webpack-merge": "^4.2.1",
        "xml-loader": "^1.2.1"
    },
    "dependencies": {
        "@babel/polyfill": "^7.12.1",
        "normalize.css": "^8.0.1",
        "vue": "^2.6.11"
    }
}
